{
  "name": "ssec/kintone-api-connect",
  "description": "A PHP wrapper for the Kintone REST API",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Shohei Tanaka",
      "email": "shoheit@ssec.shop"
    }
  ],
  "minimum-stability": "dev",
  "keywords": [
    "kintone",
    "API"
  ],
  "require": {
    "php": ">= 8.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "squizlabs/php_codesniffer": "3.*",
    "overtrue/phplint": "7.4.x-dev"
  },
  "autoload": {
    "psr-4": {
      "SoftSteps\\KintoneAPI\\": ["src/Kintone-API"]
    }
  }
  "autoload-dev": {
    "psr-4": {}
  }
}
